<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dezzapps.paisesapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.dezzapps.postsApp.view.AddPostsActivity"
            android:parentActivityName="com.dezzapps.postsApp.view.MainActivity"

            >

        </activity>
        <activity
            android:name="com.dezzapps.postsApp.view.ViewPostActivity"
            android:label="@string/title_activity_view_post"
            android:parentActivityName="com.dezzapps.postsApp.view.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dezzapps.postsApp.view.MainActivity" />
        </activity>
        <activity android:name="com.dezzapps.postsApp.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>